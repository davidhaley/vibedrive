{
  "name": "Vibedrive",
  "version": "0.8.0",
  "private": true,
  "author": "kareniel",
  "license": "GPLv2",
  "main": "index.js",
  "description": "Vibedrive Desktop Application",
  "scripts": {
    "start": "npm run watch",
    "postinstall": "run-s postinstall:*",
    "postinstall:rebuild": "electron-builder install-app-deps",
    "postinstall:link": "npm link node-traktor",
    "lint": "vue-cli-service lint",
    "watch": "run-p watch:*",
    "watch:client": "vue-cli-service serve src/client/main.js",
    "watch:server": "nodemon --exec electron src",
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "vue-cli-service build --dest dist/client",
    "build:server": "cp src/index.js dist/ && cp -r src/server dist/server",
    "build:electron": "build --config electron-builder.json",
    "local": "npm run local:mac",
    "local:mac": "./dist/electron/mac/Vibedrive.app/Contents/MacOS/Vibedrive"
  },
  "browser": {
    "stream": "stream-browserify",
    "path": "path-browserify",
    "request": "xhr"
  },
  "dependencies": {
    "concat-stream": "^1.6.2",
    "cp": "^0.2.0",
    "create-symlink": "^1.0.0",
    "debug": "^3.1.0",
    "deep-freeze": "0.0.1",
    "dropbox": "^4.0.3",
    "electron-is-dev": "^0.3.0",
    "electron-updater": "^2.21.10",
    "electron-util": "^0.9.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^2.0.0",
    "inherits": "^2.0.3",
    "isomorphic-fetch": "^2.2.1",
    "local-storage": "^1.4.2",
    "material-design-icons-iconfont": "^3.0.3",
    "music-metadata": "^0.11.4",
    "musicmetadata": "^2.0.5",
    "mv": "^2.1.1",
    "node-traktor": "^0.2.0",
    "once": "^1.4.0",
    "path-browserify": "0.0.0",
    "pouchdb": "^6.4.3",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-find": "^6.4.3",
    "query-string": "^6.0.0",
    "register-service-worker": "^1.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "run-parallel": "^1.1.9",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "socket.io-stream": "^0.9.1",
    "soundcloud": "^3.3.0",
    "stream-browserify": "^2.0.1",
    "stream-to-blob-url": "^2.1.0",
    "trash": "^4.3.0",
    "update-electron-app": "^1.2.0",
    "util-promisify": "^2.1.0",
    "uuid": "^3.2.1",
    "vue": "^2.5.16",
    "vue-pouch": "0.0.23",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^0.11.7",
    "vuetify": "^1.0.17",
    "vuex": "^3.0.1",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "@vue/eslint-config-standard": "^3.0.0-beta.9",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "standard": "^11.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
