<?xml version='1.0' encoding='utf-8'?>
<widget id="co.vibedrive.mobile" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vibedrive</name>
    <description>
        Vibedrive Mobile App.
    </description>
    <author email="hello@jonathandupre.com" href="http://jonathandupre.com">
        Jonathan Dupr√©
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
</widget>
